{
    "name": "acseo/sylius-prometheus-metrics-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "sylius-observability",
        "sylius-metrics",
        "sylius-prometheus"
    ],
    "description": "Sylius Prometheus Metrics Plugin",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sylius/sylius": "~1.11.0 || ~1.12.0",
        "artprima/prometheus-metrics-bundle": "~1.16.0 || ~1.17.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ACSEO\\SyliusPrometheusMetricsPlugin\\": "src/",
            "Tests\\ACSEO\\SyliusPrometheusMetricsPlugin\\": "tests/"
        }
    }
}
